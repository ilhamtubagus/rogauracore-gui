{
  "name": "rog-aura-gui",
  "description": "ROG aura core gui for ubuntu",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Ilham Tubagus Arfian",
    "email": "ilhamta27@gmail.com",
    "url": "https://github.com/ilhamtubagus"
  },
  "devDependencies": {
    "concurrently": "^6.3.0",
    "electron": "^15.1.0",
    "electron-builder": "^22.11.7",
    "wait-on": "^6.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mui/icons-material": "^5.0.1",
    "@mui/material": "^5.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-colorful": "^5.5.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "use-debounce": "^7.0.0",
    "web-vitals": "^1.1.2",
    "electron-is-dev": "^2.0.0"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"BROWSER=none npm run react-start\" \"npm:electron\"",
    "electron": "wait-on tcp:3000 && electron .",
    "electron-build": "electron-builder",
    "build": "npm run react-build && electron-builder -c.extraMetadata.main=build/electron.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "github.com/ilhamtubagus/rog-aura-gui-ubuntu",
    "productName": "ROG Aura Ubuntu",
    "extends": null,
    "copyright": "Copyright Â© 2019 ${author}",
    "linux": {
      "target": [
        "deb"
      ],
      "category": "Settings"
    },
    "files": [
      "build/**/*",
      "package.json"
    ],
    "publish": {
      "provider": "github",
      "repo": "rog-aura-gui-ubuntu",
      "owner": "ilhamtubagus"
    },
    "extraResources": [
      "./bin/**"
    ]
  }
}
